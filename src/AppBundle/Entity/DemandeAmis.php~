<?php

namespace AppBundle\Entity;

/**
 * DemandeAmis
 */
class DemandeAmis
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var int
     */
    private $idDemandeur;

    /**
     * @var int
     */
    private $idReceveur;

    /**
     * @var int
     */
    private $valide;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idDemandeur
     *
     * @param integer $idDemandeur
     *
     * @return DemandeAmis
     */
    public function setIdDemandeur($idDemandeur)
    {
        $this->idDemandeur = $idDemandeur;

        return $this;
    }

    /**
     * Get idDemandeur
     *
     * @return int
     */
    public function getIdDemandeur()
    {
        return $this->idDemandeur;
    }

    /**
     * Set idReceveur
     *
     * @param integer $idReceveur
     *
     * @return DemandeAmis
     */
    public function setIdReceveur($idReceveur)
    {
        $this->idReceveur = $idReceveur;

        return $this;
    }

    /**
     * Get idReceveur
     *
     * @return int
     */
    public function getIdReceveur()
    {
        return $this->idReceveur;
    }

    /**
     * Set valide
     *
     * @param integer $valide
     *
     * @return DemandeAmis
     */
    public function setValide($valide)
    {
        $this->valide = $valide;

        return $this;
    }

    /**
     * Get valide
     *
     * @return int
     */
    public function getValide()
    {
        return $this->valide;
    }
}

